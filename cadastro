		leia(rg)
		escreva("digite sua idade: ")
		leia(idade)
		escreva("digite nome do seu pai: ")
		leia(nomedopai)
		escreva("escreva nome da sua mãe: ")
      leia(nomedamae)
		escreva("digite seu peso: ")
		leia(peso)
		escreva("digte sua renda bruta: ")
		leia(rendabruta)

      escreval ("Nome ", nome , " endereço ", " cidade ", cidade , " CPF ", cpf , " RG ", rg , " idade ", idade , " nome do pai ", nomedopai , " nome da mãe ", nomedamae , " peso ", peso , " renda bruta ", rendabruta)

